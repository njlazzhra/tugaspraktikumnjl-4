program PerkalianAngka;
uses crt;
var 
    i,a,b,c:integer;

begin
    clrscr;
    write('Masukkan bilangan antara 1-100 : ');
    readln(a);
    if (a<1) or (a>100) then
        write('Baca ulang perintah ygy!')
    else
    begin
        for i := 1 to a do
        begin
            b := 1 * i;
            if (b mod 15<>0) then
            begin
                writeln(b,' ');
            end;
        end;
    end;
    readln;
end.
